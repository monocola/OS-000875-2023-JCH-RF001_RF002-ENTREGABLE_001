{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "servir-talento": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "aot": true,
            "preserveSymlinks": true,
            "outputPath": "dist/talento-orh-web",
            "index": "src/index.html",
            "main": "src/main.ts",
            "tsConfig": "tsconfig.app.json",
            "polyfills": "src/polyfills.ts",
            "assets": [
              "src/assets",
              "src/favicon.ico",
              "src/favicon.png",
              {
                "glob": "**/*",
                "input": "node_modules/ngx-extended-pdf-viewer/assets/",
                "output": "/assets/"
              }
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "node_modules/nebular-icons/scss/nebular-icons.scss",
              "node_modules/pace-js/templates/pace-theme-flash.tmpl.css",
              "src/app/@presentation/@theme/styles/styles.scss",
              "src/assets/fonts/servir-talento/style.css",
              "src/assets/realperson/jquery.realperson.css",
              "node_modules/quill/dist/quill.core.css",
              "node_modules/quill/dist/quill.snow.css",
              "node_modules/primeng/resources/themes/saga-blue/theme.css",
              "node_modules/primeicons/primeicons.css",
              "node_modules/primeng/resources/primeng.min.css"
            ],
            "scripts": [
              "node_modules/pace-js/pace.min.js",
              "node_modules/echarts/dist/echarts.min.js",
              "node_modules/echarts/dist/extension/bmap.min.js",
              "src/assets/realperson/jquery.slim.js",
              "src/assets/realperson/jquery.plugin.js",
              "src/assets/realperson/jquery.realperson.js",
              "node_modules/bootstrap/js/dist/util.js",
              "node_modules/bootstrap/js/dist/alert.js",
              "node_modules/bootstrap/js/dist/button.js",
              "node_modules/bootstrap/js/dist/carousel.js",
              "node_modules/bootstrap/js/dist/collapse.js",
              "node_modules/bootstrap/js/dist/dropdown.js",
              "node_modules/bootstrap/js/dist/modal.js",
              "node_modules/bootstrap/js/dist/scrollspy.js",
              "node_modules/bootstrap/js/dist/tab.js",
              "node_modules/bootstrap/js/dist/tooltip.js",
              "node_modules/bootstrap/js/dist/popover.js",
              "node_modules/quill/dist/quill.js",
              "node_modules/chart.js/dist/Chart.js"
            ],
            "allowedCommonJsDependencies": [
              "echarts",
              "lodash",
              "zrender/lib/svg/svg",
              "zrender/lib/vml/vml",
              "file-saver",
              "panzoom",
              "css-element-queries"
            ],
            "stylePreprocessorOptions": {
              "includePaths": ["src/app/@presentation/@theme/styles"]
            }
          },
          "configurations": {
            "production": {
              "budgets": [
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "10kb"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "fileReplacements": [
                {
                  "replace": "src/environments/environment.ts",
                  "with": "src/environments/environment.prod.ts"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "servir-talento:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "servir-talento:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "servir-talento:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "karmaConfig": "./karma.conf.js",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "scripts": [
              "node_modules/pace-js/pace.min.js",
              "node_modules/echarts/dist/echarts.min.js",
              "node_modules/echarts/dist/extension/bmap.min.js"
            ],
            "styles": [
              "./node_modules/@angular/material/prebuilt-themes/indigo-pink.css",
              "node_modules/bootstrap/dist/css/bootstrap.css",
              "node_modules/nebular-icons/scss/nebular-icons.scss",
              "node_modules/pace-js/templates/pace-theme-flash.tmpl.css",
              "src/app/@presentation/@theme/styles/styles.scss"
            ],
            "assets": ["src/assets", "src/favicon.ico", "src/favicon.png"]
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": ["**/node_modules/**"]
          }
        }
      }
    },
    "servir-talento-e2e": {
      "root": "e2e",
      "sourceRoot": "",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "./protractor.conf.js",
            "devServerTarget": "servir-talento:serve"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": ["e2e/tsconfig.json"],
            "exclude": []
          }
        }
      }
    }
  },
  "defaultProject": "servir-talento",
  "schematics": {
    "@schematics/angular:component": {
      "prefix": "serv-talento",
      "style": "scss",
      "skipTests": true
    },
    "@schematics/angular:module": {
      "prefix": "serv-talento"
    }
  },
  "cli": {
    "analytics": false
  }
}
